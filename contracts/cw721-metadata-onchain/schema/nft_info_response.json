{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NftInfoResponse",
  "type": "object",
  "properties": {
    "extension": {
      "description": "You can add any custom metadata here when you extend cw721-base",
      "anyOf": [
        {
          "$ref": "#/definitions/Metadata"
        },
        {
          "type": "null"
        }
      ]
    },
    "token_uri": {
      "description": "Universal resource identifier for this NFT Should point to a JSON file that conforms to the ERC721 Metadata JSON Schema",
      "type": [
        "string",
        "null"
      ]
    }
  },
  "definitions": {
    "Attribute": {
      "type": "object",
      "required": [
        "name",
        "value"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "Creator": {
      "type": "object",
      "required": [
        "name",
        "wallet_address"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "wallet_address": {
          "type": "string"
        }
      }
    },
    "Metadata": {
      "type": "object",
      "properties": {
        "attributes": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Attribute"
          }
        },
        "creator": {
          "anyOf": [
            {
              "$ref": "#/definitions/Creator"
            },
            {
              "type": "null"
            }
          ]
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "external_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    }
  }
}
